# Basic syntax and Operations

# Hello World
print("Hello, World!")


# Variables and Data Types

name = "Carlos"  # string
age = 39  # Integer
height = 5.9  # Float
is_student = False  # Boolean


# Operators

# Arithmetic
a = 10 + 5
b = 10 - 5
c = 10 * 5
d = 10 / 5

# Comparison
x = 10 > 5
y = 10 == 10
z = 10 != 5

# Logical
and_op = True and False
or_op = True or False

# Assignment
num = 10
num += 5
